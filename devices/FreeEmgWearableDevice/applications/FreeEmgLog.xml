<application>

    <!-- Generic information-->
    <name>FreeEmgLog</name>
    <description>Log the EMG signals</description>
    <version>0.0.1</version>

    <authors>
        <author email="riccardo.grieco@hotmail.com"> Riccardo Grieco </author>
    </authors>

    <module>
        <name>yarprobotinterface</name>

<application>

  <!-- Generic information-->
  <name>EmgOnlineForceEstimation</name>
  <description>Launch online muscle force estimation </description>
  <version>0.0.1</version>

  <authors>
      <author email="riccardo.grieco@hotmail.com"> Riccardo Grieco </author>
  </authors>

  <application>
    <name>iFeelEMGRightLegKinematics</name>
  </application>

  <module>
    <name>yarprobotinterface</name>
    <parameters>--config FreeEMG.xml</parameters>
  </module>

  <!-- Required modules. -->
  <module>
      <name>matlab</name>
      <parameters>-softwareopengl -batch "cd(getenv('FREE_EMG_MATLAB_DIR')); main_muscle"</parameters>
      <node>localhost</node>
      <dependencies>
          <port timeout="5.0">/FreeEmg/emg/r_tib_ant</port>
          <port timeout="5.0">/FreeEmg/emg/r_gastro_lat</port>
          <port timeout="5.0">/FreeEmg/emg/r_gastro_med</port>
          <port timeout="5.0">/HDE/HumanStateWrapper/state:o</port>
      </dependencies>
  </module>

</application>
